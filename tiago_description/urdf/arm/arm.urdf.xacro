<robot xmlns:xacro="http://ros.org/wiki/xacro">

  <!--File includes-->
  <xacro:include filename="$(find tiago_description)/urdf/deg_to_rad.xacro" />
  <xacro:include filename="$(find tiago_description)/urdf/arm/wrist.urdf.xacro" />
  <xacro:include filename="$(find tiago_description)/urdf/arm/arm.transmission.xacro" />

  <!--Constant parameters-->
  <xacro:property name="arm_friction"       value="1.0" />
  <xacro:property name="arm_damping"        value="1.0" />
  <xacro:property name="arm_1_max_vel"      value="2.7" />
  <xacro:property name="arm_2_max_vel"      value="3.66" />
  <xacro:property name="arm_3_max_vel"      value="4.58" />
  <xacro:property name="arm_4_max_vel"      value="4.58" />
  <xacro:property name="arm_1_max_effort"   value="39.0" />
  <xacro:property name="arm_2_max_effort"   value="39.0" />
  <xacro:property name="arm_3_max_effort"   value="17.86" />
  <xacro:property name="arm_4_max_effort"   value="17.86" />
  <xacro:property name="arm_eps"            value="0.02" />


  <!-- TODO: Update dynamic models -->

  <xacro:macro name="tiago_arm" params="name parent">

    <!--************************-->
    <!--        SHOULDER        -->
    <!--************************-->
    <link name="${name}_1_link">
      <inertial>
        <origin xyz="0.00 -0.00011 0.18627" rpy="0 0 0"/>
        <mass value="1.53614" />
        <inertia ixx="0.00327313329"  ixy="-0.00000460054"  ixz="-0.00025121368" iyy="0.00316047802" iyz="0.00000313158" izz="0.00157642528" />

      </inertial>
      <!-- M90 module -->
      <visual>
        <origin xyz="0 0 -0.0425" rpy="0 0 0" />
        <geometry>
          <cylinder length="0.085" radius="0.045" />
        </geometry>
        <material name="DarkGrey" />
      </visual>
      <collision>
        <origin xyz="0 0 -0.0425" rpy="0 0 0" />
        <geometry>
          <cylinder length="0.085" radius="0.045" />
        </geometry>
        <material name="DarkGrey" />
      </collision>
      <!-- Connecting plate -->
      <visual>
        <origin xyz="0.05 0 0.01" rpy="0 0 0" />
        <geometry>
          <box size="0.2 0.09 0.02" />
        </geometry>
        <material name="Blue" />
      </visual>
      <collision>
        <origin xyz="0 0 -0.0425" rpy="0 0 0" />
        <geometry>
          <cylinder length="0.02" radius="0.045" />
        </geometry>
        <material name="Blue" />
      </collision>
      <!-- Root of arm -->
      <visual>
        <origin xyz="0.12 0 -0.107" rpy="${-90 * deg_to_rad} 0 ${180 * deg_to_rad}" />
        <geometry>
          <mesh filename="package://tiago_description/meshes/arm/arm_2.dae" scale="1 1 1"/>
        </geometry>
        <material name="DarkGrey" />
      </visual>
      <collision>
       <origin xyz="0.12 0 -0.107" rpy="${-90 * deg_to_rad} 0 ${180 * deg_to_rad}" />
        <geometry>
          <mesh filename="package://tiago_description/meshes/arm/arm_2_collision.stl" scale="1 1 1"/>
        </geometry>
      </collision>
    </link>

    <joint name="${name}_1_joint" type="revolute">
      <parent link="${parent}" />
      <child link="${name}_1_link" />
      <origin xyz="0.157 0.064625 0" rpy="${0 * deg_to_rad} ${0 * deg_to_rad} ${-90 * deg_to_rad}" />
      <axis xyz="0 0 1" />
      <limit lower="${0.0 * deg_to_rad}" upper="${180.0 * deg_to_rad}" effort="${arm_1_max_effort}" velocity="${arm_1_max_vel}" />
      <dynamics friction="${arm_friction}" damping="${arm_damping}"/>

      <safety_controller k_position="20"
                         k_velocity="20"
                         soft_lower_limit="${0 * deg_to_rad  + arm_eps}"
                         soft_upper_limit="${180.0 * deg_to_rad - arm_eps}" />
    </joint>

   <link name="${name}_2_link">
      <inertial>
        <origin xyz="0.01435 -0.06294 ${0.01039}" rpy="0 0 0"/>
        <mass value="1.60393" />
        <inertia ixx="0.0046195621"  ixy="-0.00056769245"  ixz="0.00018376961" iyy="0.00185384237" iyz="-0.00078286958" izz="0.0049356022" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="${180 * deg_to_rad} ${0 * deg_to_rad} ${-90 * deg_to_rad}" />
        <geometry>
          <mesh filename="package://tiago_description/meshes/arm/arm_2.dae" scale="1 1 1"/>
        </geometry>
        <material name="DarkGrey" />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="${180 * deg_to_rad} ${0 * deg_to_rad} ${-90 * deg_to_rad}" />
        <geometry>
          <mesh filename="package://tiago_description/meshes/arm/arm_2_collision.stl" scale="1 1 1"/>
        </geometry>
      </collision>
    </link>

    <joint name="${name}_2_joint" type="revolute">
      <parent link="${name}_1_link" />
      <child link="${name}_2_link" />
      <origin xyz="0.12 0.006 -0.107" rpy="${90.0 * deg_to_rad} ${0 * deg_to_rad} ${0 * deg_to_rad}" />
      <axis xyz="0 0 1" />
      <limit lower="${-112.5 * deg_to_rad}" upper="${22.5 * deg_to_rad}" effort="${arm_2_max_effort}" velocity="${arm_2_max_vel}" />
      <dynamics friction="${arm_friction}" damping="${arm_damping}"/>

      <safety_controller k_position="20"
                         k_velocity="20"
                         soft_lower_limit="${-112.5 * deg_to_rad  + arm_eps}"
                         soft_upper_limit="${ 22.5 * deg_to_rad - arm_eps}" />
    </joint>

    <link name="${name}_3_link">
      <inertial>
        <origin xyz="0.00606 -0.0113 0.05748" rpy="0 0 0"/>
        <mass value="1.64293" />
         <inertia ixx="0.00372686652"  ixy="-0.00008722874"  ixz="-0.00028800176" iyy="0.00344463026" iyz="0.00042776588" izz="0.00192229026" />
      </inertial>
      <visual>
        <origin xyz="0 0 -0.1" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://tiago_description/meshes/arm/arm_3.dae" scale="1 1 1"/>
        </geometry>
        <material name="DarkGrey" />
      </visual>
      <visual>
        <origin xyz="0 0 -0.025" rpy="0 0 0" />
        <geometry>
          <cylinder length="0.11" radius="0.05" />
        </geometry>
        <material name="DarkGrey" />
      </visual>
      <collision>
        <origin xyz="0 0 -0.1" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://tiago_description/meshes/arm/arm_3_collision.stl" scale="1 1 1"/>
        </geometry>
      </collision>
      <collision>
        <origin xyz="0 0 -0.025" rpy="0 0 0" />
        <geometry>
          <cylinder length="0.11" radius="0.05" />
        </geometry>
        <material name="DarkGrey" />
      </collision>
    </link>

    <joint name="${name}_3_joint" type="revolute">
      <parent link="${name}_2_link" />
      <child link="${name}_3_link" />
      <origin xyz="0.142 -0.02 0" rpy="${0.0 * deg_to_rad} ${-90.0 * deg_to_rad} 0" />
      <axis xyz="0 0 1" />
      <limit lower="${-135.0 * deg_to_rad}" upper="${157.5 * deg_to_rad}" effort="${arm_3_max_effort}" velocity="${arm_3_max_vel}" />
      <dynamics friction="${arm_friction}" damping="${arm_damping}"/>

      <safety_controller k_position="20"
                         k_velocity="20"
                         soft_lower_limit="${-135.0 * deg_to_rad + arm_eps}"
                         soft_upper_limit="${ 157.5 * deg_to_rad - arm_eps}" />
    </joint>

    <!--************************-->
    <!--        ELBOW           -->
    <!--************************-->
    <link name="${name}_4_link">
      <inertial>
        <origin xyz="-0.04129 0.0118 -0.01387" rpy="0 0 0"/>
        <mass value="1.20222" />
        <inertia ixx="0.00156355749"  ixy="-0.000353141"  ixz="0.00053402452" iyy="0.00277186194" iyz="-0.00015541973" izz="0.00307369106" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://tiago_description/meshes/arm/arm_4.dae" scale="1 1 1"/>
        </geometry>
        <material name="DarkGrey" />
      </visual>
      <visual>
        <origin xyz="-0.15 0.02 0" rpy="0 ${90 * deg_to_rad} 0" />
        <geometry>
          <cylinder length="0.07" radius="0.05" />
        </geometry>
        <material name="DarkGrey" />
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://tiago_description/meshes/arm/arm_4_collision.stl" scale="1 1 1"/>
        </geometry>
      </collision>
      <collision>
        <origin xyz="-0.15 0.02 0" rpy="0 ${90 * deg_to_rad} 0" />
        <geometry>
          <cylinder length="0.07" radius="0.05" />
        </geometry>
        <material name="DarkGrey" />
      </collision>
    </link>

    <joint name="${name}_4_joint" type="revolute">
      <parent link="${name}_3_link" />
      <child link="${name}_4_link" />
      <origin xyz="0 -0.02 -0.142" rpy="0 ${-90.0 * deg_to_rad} 0" />
      <axis xyz="0 0 1" />
      <limit lower="${-130 * deg_to_rad}" upper="${0 * deg_to_rad}" effort="${arm_4_max_effort}" velocity="${arm_4_max_vel}" />
      <dynamics friction="${arm_friction}" damping="${arm_damping}"/>

      <safety_controller k_position="20"
                         k_velocity="20"
                         soft_lower_limit="${ -130.0 * deg_to_rad - arm_eps}"
                         soft_upper_limit="${ 0 * deg_to_rad - arm_eps}" />
    </joint>

   <gazebo reference="${name}_1_link">
     <mu1>0.9</mu1>
     <mu2>0.9</mu2>
   </gazebo>
   <gazebo reference="${name}_2_link">
     <mu1>0.9</mu1>
     <mu2>0.9</mu2>
   </gazebo>
   <gazebo reference="${name}_3_link">
     <mu1>0.9</mu1>
     <mu2>0.9</mu2>
   </gazebo>
   <gazebo reference="${name}_4_link">
     <mu1>0.9</mu1>
     <mu2>0.9</mu2>
   </gazebo>

   <gazebo reference="${name}_1_joint">
      <implicitSpringDamper>1</implicitSpringDamper>
   </gazebo>
   <gazebo reference="${name}_2_joint">
      <implicitSpringDamper>1</implicitSpringDamper>
   </gazebo>
   <gazebo reference="${name}_3_joint">
      <implicitSpringDamper>1</implicitSpringDamper>
   </gazebo>
   <gazebo reference="${name}_4_joint">
      <implicitSpringDamper>1</implicitSpringDamper>
   </gazebo>


    <!--************************-->
    <!--        WRIST           -->
    <!--************************-->
    <xacro:wrist name="arm" parent="${name}_4_link" reflect="1"/>

    <!--***********************-->
    <!--        TOOL           -->
    <!--***********************-->
    <link name="${name}_tool_link">
      <inertial>
        <mass value="0.1" />
        <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0" />
      </inertial>
      <visual>
        <origin xyz="0.001 0 0" rpy="0 ${90.0 * deg_to_rad} 0" />
        <geometry>
          <cylinder radius="0.005" length="0.005"/>
        </geometry>
        <material name="LightGrey" />
      </visual>
      <collision>
        <origin xyz="0.001 0 0" rpy="0 ${90.0 * deg_to_rad} 0" />
        <geometry>
          <cylinder radius="0.005" length="0.005"/>
        </geometry>
      </collision>
    </link>

    <joint name="${name}_tool_joint" type="fixed">
      <parent link="${name}_7_link" />
      <child link="${name}_tool_link" />
      <origin xyz="0 0 0.046" rpy="${90.0 * deg_to_rad} ${-90.0 * deg_to_rad} ${180 * deg_to_rad}" />
    </joint>


    <!-- extensions -->
    <xacro:arm_simple_transmission name="${name}" number="1" reduction="1.0" />
    <xacro:arm_simple_transmission name="${name}" number="2" reduction="1.0" />
    <xacro:arm_simple_transmission name="${name}" number="3" reduction="1.0" />
    <xacro:arm_simple_transmission name="${name}" number="4" reduction="1.0" />
  </xacro:macro>

</robot>
