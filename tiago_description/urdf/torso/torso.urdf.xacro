<?xml version="1.0" ?>

<robot xmlns:xacro="http://ros.org/wiki/xacro">

  <!--File includes-->
  <xacro:include filename="$(find tiago_description)/urdf/deg_to_rad.xacro" />

 <!--Constant parameters-->
  <xacro:property name="torso_max_vel"  value="5.4" />
  <xacro:property name="torso_max_effort"  value="78.0" />
  <xacro:property name="torso_eps"      value="0.02" />

  <xacro:macro name="tiago_torso" params="name parent">

  <link name="${name}_fixed_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0.00635 -0.01673 0.10662" />
      <mass value="14.52876" />
      <inertia ixx="0.17621875653" ixy="-0.00154333947"   ixz="0.00980172504"
               iyy="0.18995293862" iyz="-0.0259201152"
               izz="0.03571737542" />
    </inertial>

  <visual>
      <origin rpy="0 0 0" xyz="0 0 0.2235" />
      <geometry>
        <box size="0.14 0.1 0.463"/>
      </geometry>
      <material name="LightGrey"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.2315" />
      <geometry>
        <box size="0.14 0.1 0.463"/>
      </geometry>
      <material name="LightGrey"/>
    </collision>
  </link>

  <joint name="${name}_fixed_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-0.068 0 0.2235" />
    <parent link="${parent}" />
    <child link="${name}_fixed_link" />
    <axis xyz="0 0 0" />
  </joint>

  <link name="${name}_lift_link">
    <inertial>
      <origin rpy="0 0 0" xyz="0.02909 0.01068 -0.14826" />
      <mass value="8.62177" />
      <inertia ixx="0.19639986797" ixy="0.00528625771" ixz="-0.04018414421"
               iyy="0.212736527" iyz="-0.01564349386"
               izz="0.02479371908" />
    </inertial>
    <!-- Lifting body -->
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.25125" />
       <geometry>
        <box size="0.16 0.106 0.5025"/>
      </geometry>
      <material name="White"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -0.25125" />
       <geometry>
        <box size="0.16 0.106 0.5025"/>
      </geometry>
      <material name="White"/>
    </collision>
    <!-- Head pan motor -->
    <visual>
      <origin xyz="0.187 0 0.02" rpy="0 0 0"/>
      <geometry>
        <box size="0.06 0.04 0.04"/>
      </geometry>
      <material name="Grey" />
    </visual>
    <collision>
      <origin xyz="0.187 0 0.02" rpy="0 0 0" />
      <geometry>
        <box size="0.06 0.04 0.04"/>
      </geometry>
      <material name="Grey" />
    </collision>

  </link>

  <joint name="${name}_lift_joint" type="prismatic">
    <origin rpy="0 0 0" xyz="0.0 0 0.5025" />
    <parent link="${name}_fixed_link" />
    <child link="${name}_lift_link" />
    <axis xyz="0 0 1" />
    <limit effort="2000" lower="0.0" upper="0.35" velocity="0.01" />
    <calibration rising="0.0" />
    <dynamics damping="1000" />
  </joint>

 </xacro:macro>

</robot>
